	<!-- Node definitions.  These appear as a tree in eclipse -->
	
	<!-- Generic Nodes -->	
	<node id="ProgramMemmoryLoadNode">
		<name>Program/Memory Load Options</name>
		<children>
			<child>ProgramLoadGroup</child>
			<child>ConnectGroup</child>
			<child>DisableInterrupts</child>
			<child>ResetOnRestart</child>
			<child>RestartOnSymbolLoad</child>
		</children>
	</node>
	
	<node id="AutoRunAndLaunchNode">
