		<category read="true">Device Info</category>
	</categories>
	
	<!-- Property definitions -->

	<!-- Generic Property Definitions -->
	<property id="ClearBreakpointsOnLoad">
		<target>generic</target>
		<name>Disable all breakpoints when loading a different program</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
	</property>
	
	<property id="AddCEXITbreakpointAfterLoad">
		<target>generic</target>
		<name>Halt at program exit for TI compilers (requires a breakpoint)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<uniqueDefault value="0">
			<target>MSP430</target>
			<target>TMS320C28XX</target>
			<target>TMS192C20XX</target>
			<noSystemBreakpoints>true</noSystemBreakpoints>
		</uniqueDefault>
	</property>
	
	<property id="AddCIOBreakpointAfterLoad">
		<target>generic</target>
		<name>Enable CIO function use (requires setting a breakpoint)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<uniqueDefault value="0">
			<target>TMS192C20XX</target>
			<noSystemBreakpoints>true</noSystemBreakpoints>
		</uniqueDefault>
	</property>
	
	<property id="AddSemiHostingBreakpointAfterLoad">
		<target>TMS470R1X</target>
		<target>TMS470R2X</target>
		<target>TMS470R3X</target>
		<target>TMS470RCX</target>
		<target>TMS470R9X</target>
		<target>TMS470R15X</target>
		<target>TMS470R53X</target>
		<target>TMS470RDX</target>
		<target>TMS470REX</target>
		<name>Enable Semihosting (requires setting a breakpoint at SVC_Handler )</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<uniqueDefault value="0">
			<noSystemBreakpoints>true</noSystemBreakpoints>
		</uniqueDefault>
	</property>
	
	<property id="PreventStepIntoAsm">
		<target>generic</target>
		<name>Automatically step over functions without debug information when source stepping</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
	</property>
	
	<property id="ResetOnRestart">
		<target>generic</target>
		<name>Reset the target on a program load or restart</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
		<uniqueDefault value="1">
			<target>MSP430</target>
			<msp432fet>true</msp432fet>
		</uniqueDefault>
	</property>
	
	<property id="RestartOnSymbolLoad">
		<target>generic</target>
		<name>Restart the target on a symbol load as well as a program load</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
		<uniqueDefault value="1">
			<msp432fet>true</msp432fet>
		</uniqueDefault>
	</property>

	<!-- MSP devices ALWAYS do a low power run - there's no other way.  As
		 such, the option is set to "checked", but disabled so it can't be 
		 unchecked. -->
	<property id="LowPowerRunMode">
		<target>generic</target>
		<name>Allow power transitions while running if supported (low power running)</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
		<uniqueDefault value="1">
			<lowPowerRunByDefault>true</lowPowerRunByDefault>
			<target>MSP430</target>
		</uniqueDefault>
		<disableIf>
			<target>MSP430</target>
		</disableIf>
	</property>
	
	<property id="CIOTimestamp">
		<target>generic</target>
		<name>Add timestamp information to target output</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
	</property>
	
	<property id="EnableSoftwareBreakpoints">
		<target>generic</target>
		<name>Allow software breakpoints to be used</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<uniqueDefault value="0">
			<eem>EMEX_HIGH</eem>
			<disableSoftwareBreakpoint>true</disableSoftwareBreakpoint>
		</uniqueDefault>
		<disableIf>
			<disableSoftwareBreakpoint>true</disableSoftwareBreakpoint>
		</disableIf>
	</property>
	
	<property id="FileIODefaultDirectory">
		<target>generic</target>
		<name>Default directory for File IO: </name>
		<valueType>String</valueType>
		<defaultValue></defaultValue>
	</property>
	
	<property id="SyncBreakpointsAndSymbols">
		<target>generic</target>
		<name>When added to a sync group:</name>
		<valueType>ChoiceList</valueType>
		<values>
			<value>Synchronize execution only</value>
			<value>Synchronize breakpoints and symbols (with like cores), as well as execution</value>
		</values>
		<defaultValue>Synchronize breakpoints and symbols (with like cores), as well as execution</defaultValue>
	</property>
	
	<property id="VerifyAfterProgramLoad">
		<target>generic</target>
		<name>Verification Options</name>
		<valueType>ChoiceList</valueType>
		<values>
			<value>Full verification</value>
			<value>Fast verification</value>
			<value>No verification</value>
		</values>
		<defaultValue>Fast verification</defaultValue>
		<uniqueDefault value="Full verification">
			<target>MSP430</target>
		</uniqueDefault>
	</property>
	
	<property id="UseLegacyStopMode">
		<target>generic</target>
		<name>Halt the target before any debugger access (will impact servicing of interrupts)</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
	</property>
			
	<property id="AllowInterruptsWhenHalted">
		<target>generic</target>
		<name>Enable realtime mode (critical interrupts serviced when halted, rude/polite mode...)</name>
		<valueType>Boolean</valueType>
		<defaultValue>0</defaultValue>
		<enableIf>
			<realtimeEnabled>true</realtimeEnabled>
			<supportsAdvancedRealtime>true</supportsAdvancedRealtime>
		</enableIf>
	</property>
	
	<property id="PoliteRealtimeMode">
		<target>generic</target>
		<name>Enable polite mode (respect HPI, DBGM and FRAMEID)</name>
		<valueType>Boolean</valueType>
		<defaultValue>1</defaultValue>
		<uniqueDefault value="0">
			<supportsAdvancedRealtime>true</supportsAdvancedRealtime>
		</uniqueDefault>
		<enableIf>
			<supportsAdvancedRealtime>true</supportsAdvancedRealtime>
			<property id="AllowInterruptsWhenHalted">1</property>
		</enableIf>
	</property>
	
	<property id="ModuleDebuggingEnabled">
		<target>generic</target>
		<name>Automatically load module symbols</name>
		<valueType>ChoiceList</valueType>
		<values>
			<value>Never</value>
			<value>When modules are loaded and unloaded (intrusive on target execution, and requires a patched kernel)</value>
			<value>Whenever halted in the module load/unload routines (you must manually set breakpoints in the appropriate routines for this to work)</value>
		</values>
		<defaultValue>Never</defaultValue>
	</property>

  <property id="ModuleNameList">
    <target>generic</target>
